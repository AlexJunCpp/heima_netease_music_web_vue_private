e=\"text\"\r\n          autocomplete=\"off\"\r\n          placeholder=\"搜索\"\r\n          class=\"el-input__inner\"\r\n          v-model=\"inputValue\"\r\n          @keyup.enter=\"toResult\"\r\n        />\r\n        <span class=\"el-input__prefix\">\r\n          <i class=\"el-input__icon el-icon-search\"></i>\r\n        </span>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  export default {\r\n    name: 'top',\r\n    data() {\r\n      return {\r\n        // 输入的内容\r\n        inputValue: ''\r\n      }\r\n    },\r\n    methods: {\r\n      toResult() {\r\n        // 非空判断\r\n        if (this.inputValue == '') {\r\n          // 提示用户\r\n          this.$message.warning('请输入内容')\r\n        }else{\r\n          // 去搜索页 携带数据\r\n          this.$router.push('/result?q='+this.inputValue)\r\n        }\r\n      }\r\n    }\r\n  }\r\n</script>\r\n\r\n<style scoped></style>\r\n"]}]}